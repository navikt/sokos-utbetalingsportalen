---
import ContentLoader from "@components/loader/ContentLoader";
import MicrofrontendSSR from "@components/microfrontend/MicrofrontendSSR.astro";
import Layout from "@layouts/Layout.astro";
---

<Layout title="Skattekort ssr">
  <MicrofrontendSSR
    appTitle="Skattekort ssr"
    appUrl={process.env.SOKOS_SKATTEKORT_SSR_URL}
    appAudience={process.env.SOKOS_SKATTEKORT_SSR_AUDIENCE}
    server:defer
  >
    <ContentLoader slot="fallback" />
  </MicrofrontendSSR>
</Layout>