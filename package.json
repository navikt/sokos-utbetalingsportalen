		"@navikt/ds-react": "7.36.0",
		"@navikt/oasis": "^4.1.1",
		"@opentelemetry/sdk-node": "^0.208.0",
		"astro": "^5.16.6",
		"pino": "^10.1.0",
		"pino-socket": "^7.4.0",
		"prom-client": "^15.1.3",
		"react": "19.2.0",
		"react-dom": "19.2.0",
		"react-error-boundary": "^6.0.1",
		"swr": "2.3.8",
		"zod": "^4.2.1"
